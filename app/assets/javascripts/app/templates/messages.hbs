<div class="container">
  <div class="row">

    <div class="col-sm-10">
      <div id="messages">
        <ul>
          {{#each}}
            {{render message this}}
          {{/each}}
        </ul>
      </div>
    </div>
  
    <div class="col-sm-2">
      <button {{action newMessage}} class="btn btn-lg btn-primary">New Message</button>

      <p>Online Users</p>
    </div>

  </div>
</div>